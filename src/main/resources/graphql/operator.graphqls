type Operator{
    id: ID!
    name: String!
    email: String!
    login: String!
    password: String!
    active: Boolean!
    logged: Boolean!
    loggedIn: String
    loggedOut: String
    pass: String!
}

type TypeUser{
    id: ID!
    functionOperator: String!
}

type Query{
    login(login: String!, password: String!) : Operator
    logout(id: ID!, pass: String!): Operator
}

type Mutation{
    setDataOperator(idOperator: ID!, pass: String!, action: Int!) : Operator
}
